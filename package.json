{
  "name": "ember-cli-deploy-fastboot-s3",
  "version": "0.3.2",
  "description": "An ember-cli-deploy plugin that archives and uploads FastBoot build to AWS S3.",
  "keywords": [
    "ember-addon",
    "ember-cli-deploy-plugin",
    "ember-cli-deploy-fastboot-s3",
    "ember-cli-fastboot"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "npm run test:node && npm run test:ember",
    "test:ember": "ember try:each",
    "test:node": "node ./tests/runner.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/he9qi/ember-cli-deploy-fastboot-s3.git"
  },
  "author": "Qi He <qhe@heyook.com>",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.6.1",
    "ember-cli-babel": "^5.1.7",
    "ember-cli-deploy-plugin": "^0.2.6",
    "fs-extra": "^0.30.0",
    "fs-promise": "^0.5.0",
    "archiver": "^1.3.0"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "ember-ajax": "^2.4.1",
    "ember-cli": "2.12.0",
    "ember-cli-app-version": "^1.0.0",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-eslint": "^3.0.0",
    "ember-cli-htmlbars": "^1.1.1",
    "ember-cli-htmlbars-inline-precompile": "^0.3.6",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^3.1.0",
    "ember-cli-release": "^0.2.9",
    "ember-cli-shims": "^1.0.2",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-test-loader": "^1.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-data": "^2.7.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^1.0.5",
    "ember-load-initializers": "^0.6.0",
    "ember-resolver": "^2.0.3",
    "ember-source": "~2.12.0",
    "eslint-plugin-mocha": "^4.9.0",
    "glob": "^7.0.6",
    "loader.js": "^4.2.3",
    "mocha": "^3.2.0",
    "unzip": "^0.1.11"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-cli-deploy-build",
      "ember-cli-deploy-revision-data"
    ]
  },
  "engines": {
    "node": ">= 4"
  }
}
